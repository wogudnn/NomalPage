<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean	id="boardService"
			class="com.ktdsuniv.board.service.impl.BoardServiceImpl">
		<property name="boardBiz" ref="boardBiz"/>		
	</bean>

	<bean	id="boardBiz"
			class="com.ktdsuniv.board.biz.impl.BoardBizImpl">
		<property name="boardDao" ref="boardDao"/>		
	</bean>
	
	<bean	id="boardDao"
			class="com.ktdsuniv.board.dao.impl.BoardDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

	<bean	id="replyService"
			class="com.ktdsuniv.board.service.impl.ReplyServiceImpl">
		<property name="replyBiz" ref="replyBiz"/>		
	</bean>
	
	<bean	id="replyBiz"
			class="com.ktdsuniv.board.biz.impl.ReplyBizImpl">
		<property name="replyDao" ref="replyDao"/>		
	</bean>
	
	<bean	id="replyDao"
			class="com.ktdsuniv.board.dao.impl.ReplyDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

</beans>
