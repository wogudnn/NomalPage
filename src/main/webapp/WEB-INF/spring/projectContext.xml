<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean	id="projectService"
			class="com.ktdsuniv.project.service.impl.ProjectServiceImpl">
		<property name="projectBiz" ref="projectBiz"/>		
	</bean>
	
	<bean	id="projectBiz"
			class="com.ktdsuniv.project.biz.impl.ProjectBizImpl">
		<property name="projectDao" ref="projectDao"/>		
	</bean>
	
	<bean	id="projectDao"
			class="com.ktdsuniv.project.dao.impl.ProjectDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

</beans>
