<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean	id="roomService"
			class="com.ktdsuniv.room.service.impl.RoomServiceImpl">
		<property name="roomBiz" ref="roomBiz"/>		
	</bean>

	<bean	id="roomBiz"
			class="com.ktdsuniv.room.biz.impl.RoomBizImpl">
		<property name="roomDao" ref="roomDao"/>		
	</bean>

	<bean	id="roomDao"
			class="com.ktdsuniv.room.dao.impl.RoomDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

	<bean	id="seatService"
			class="com.ktdsuniv.room.service.impl.SeatServiceImpl">
		<property name="seatBiz" ref="seatBiz"/>		
	</bean>
	
	<bean	id="seatBiz"
			class="com.ktdsuniv.room.biz.impl.SeatBizImpl">
		<property name="seatDao" ref="seatDao"/>		
	</bean>
	
	<bean	id="seatDao"
			class="com.ktdsuniv.room.dao.impl.SeatDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

</beans>
