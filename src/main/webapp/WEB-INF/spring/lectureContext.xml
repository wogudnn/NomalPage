<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean	id="lectureService"
			class="com.ktdsuniv.lecture.service.impl.LectureServiceImpl">
		<property name="lectureBiz" ref="lectureBiz"/>		
	</bean>

	<bean	id="lectureBiz"
			class="com.ktdsuniv.lecture.biz.impl.LectureBizImpl">
		<property name="lectureDao" ref="lectureDao"/>		
	</bean>
	
	<bean	id="lectureDao"
			class="com.ktdsuniv.lecture.dao.impl.LectureDaoImpl">
		<property name="mongoTemplate" ref="mongoTemplate"/>		
	</bean>

</beans>
